{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "dom",
      "esnext"
    ],
    "allowJs": true,
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./src/**/*"
  ]
}
